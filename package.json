{
  "name": "media-service",
  "version": "1.0.0",
  "main": "main.ts",
  "engineStrict": true,
  "engines": {
    "node": "^20.18.1"
  },
  "scripts": {
    "start": "tsx ./src/main.ts",
    "build": "node build.js",
    "dev": "tsx watch ./src/main.ts",
    "prod": "npm run build && node dist/main.js",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml --env-file .env.dev up --build",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml --env-file .env.prod up --build",
    "docker:down": "docker-compose down --volumes --remove-orphans"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/busboy": "^1.5.4",
    "@types/connect": "^3.4.38",
    "@types/finalhandler": "^1.2.4",
    "@types/node": "^22.14.1",
    "@types/qs": "^6.9.18",
    "@types/socket.io": "^3.0.1",
    "esbuild": "^0.25.6",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/lib-storage": "^3.842.0",
    "@bull-board/api": "^6.11.0",
    "@bull-board/express": "^6.11.0",
    "bullmq": "^5.56.2",
    "busboy": "^1.6.0",
    "connect": "^3.7.0",
    "dotenv": "^16.5.0",
    "finalhandler": "^2.1.0",
    "ioredis": "^5.6.1",
    "qs": "^6.14.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  }
}
